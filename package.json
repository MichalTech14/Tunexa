{
  "name": "tunexa",
  "version": "1.0.0",
  "description": "Inteligent Audio Engine - Modulárny systém pre analýzu, tuning a certifikáciu audio systémov v automobiloch",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "tunexa": "./dist/index.js"
  },
  "scripts": {
    "build": "npx tsc",
    "dev": "npx tsx index.ts",
    "start": "node dist/index.js",
    "api": "node dist/api/server.js",
    "api:start": "nohup node dist/api/server.js > /dev/null 2>&1 &",
    "api:stop": "pkill -f 'node dist/api/server.js' || true",
    "api:restart": "npm run api:stop && sleep 2 && npm run api:start",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "validate:acoustics": "npx tsx scripts/validate-acoustics.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "demo": "npm run dev",
    "server": "npx tsx server.ts",
    "server:prod": "node dist/server.js",
    "db:setup": "npx tsx database/setup.ts",
    "db:migrate": "npx tsx database/migrate.ts",
    "db:seed": "npx tsx database/seed.ts",
    "db:reset": "rm -f database/tunexa.db && npm run db:setup && npm run db:seed",
    "migrate": "npx tsx scripts/migrate.ts",
    "migrate:status": "npm run migrate status",
    "migrate:run": "npm run migrate run",
    "migrate:rollback": "npm run migrate rollback",
    "migrate:create": "npm run migrate create",
    "migrate:validate": "npm run migrate validate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichalTech14/Tunexa.git"
  },
  "keywords": [
    "audio",
    "automotive",
    "tuning",
    "acoustics",
    "car-audio"
  ],
  "author": "MichalTech14",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MichalTech14/Tunexa/issues"
  },
  "homepage": "https://github.com/MichalTech14/Tunexa#readme",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/lab": "^7.0.1-beta.18",
    "@mui/material": "^7.3.4",
    "@types/express-validator": "^3.0.2",
    "@types/ioredis": "^5.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/socket.io-client": "^3.0.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "glob": "^10.3.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "recharts": "^3.3.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.22",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.0.4",
    "chalk": "^5.6.2",
    "commander": "^12.1.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.1.11"
  }
}
