{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "npm:dev",
      "command": "npm run dev",
      "isBackground": true,
      "problemMatcher": {
        "owner": "custom",
        "fileLocation": ["relative", "${workspaceFolder}"],
        "pattern": [
          {
            "regexp": ".*",
            "file": 1,
            "location": 2,
            "message": 3
          }
        ]
      }
    }
  ]
}
